{
  "default": "A",
  "supplies": {
    "A": {
      "driver": "map",
      "description": "Keysight/Agilent E3645A (E364xA series) over RS-232 using SCPI",
      "serial": {
        "baudrate": 9600,
        "bytesize": 8,
        "parity": "N",
        "stopbits": 2,
        "timeout_s": 1.0,
        "write_timeout_s": 1.0,
        "newline": "\n"
      },
      "command_map": {
        "IDN": "*IDN?",
        "RESET": "*RST",
        "SYSTEM_REMOTE": "SYSTem:REMote",
        "SYSTEM_LOCAL": "SYSTem:LOCal",
        "SYSTEM_RWLOCK": "SYSTem:RWLock",
        "SET_RANGE_LOW": "VOLT:RANG P35V",
        "SET_RANGE_HIGH": "VOLT:RANG P60V",
        "OVP_SET": "VOLT:PROT {value}",
        "OVP_ENABLE": "VOLT:PROT:STAT ON",
        "OVP_DISABLE": "VOLT:PROT:STAT OFF",
        "OVP_CLEAR": "VOLT:PROT:CLE",
        "OPEN_OUTPUT": "OUTP ON",
        "CLOSE_OUTPUT": "OUTP OFF",
        "SET_VOLTAGE": "VOLT {value}",
        "SET_CURRENT": "CURR {value}",
        "MEASURE_VOLTAGE": "MEAS:VOLT?",
        "MEASURE_CURRENT": "MEAS:CURR?"
      },
      "expect_response": [
        "IDN",
        "MEASURE_VOLTAGE",
        "MEASURE_CURRENT"
      ]
    },
    "B": {
      "driver": "map",
      "description": "Supply B profile (placeholder - replace when B documentation is provided)",
      "serial": {
        "baudrate": 9600,
        "bytesize": 8,
        "parity": "N",
        "stopbits": 1,
        "timeout_s": 1.0,
        "write_timeout_s": 1.0,
        "newline": "\n"
      },
      "command_map": {
        "IDN": "*IDN?",
        "OPEN_OUTPUT": "OUTP ON",
        "CLOSE_OUTPUT": "OUTP OFF",
        "SET_VOLTAGE": "VOLT {value}",
        "SET_CURRENT": "CURR {value}",
        "MEASURE_VOLTAGE": "MEAS:VOLT?",
        "MEASURE_CURRENT": "MEAS:CURR?"
      },
      "expect_response": [
        "IDN",
        "MEASURE_VOLTAGE",
        "MEASURE_CURRENT"
      ]
    }
  }
}
